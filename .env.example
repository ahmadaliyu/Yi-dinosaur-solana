# Yi Dinosaur Environment Variables
# Copy this file to .env and fill in your values

# ============================================
# SOLANA CONFIGURATION
# ============================================
# Your $Yi token mint address on Solana
VITE_YI_TOKEN_MINT=YOUR_YI_TOKEN_MINT_ADDRESS

# DEX pair address (from Raydium, Orca, etc.)
VITE_YI_PAIR_ADDRESS=YOUR_DEX_PAIR_ADDRESS

# Solana RPC endpoint (use a dedicated one for production)
# Free: https://api.mainnet-beta.solana.com
# Paid: Helius, QuickNode, Alchemy
VITE_SOLANA_RPC=https://api.mainnet-beta.solana.com

# ============================================
# SOCIAL MEDIA APIs (Optional)
# ============================================
# Twitter/X API - Get from https://developer.twitter.com
VITE_TWITTER_BEARER_TOKEN=

# Telegram Bot - Create bot via @BotFather
VITE_TELEGRAM_BOT_TOKEN=
VITE_TELEGRAM_CHAT_ID=

# ============================================
# ANALYTICS (Optional)
# ============================================
# Birdeye API - Get from https://birdeye.so
VITE_BIRDEYE_API_KEY=

# Helius API - Get from https://helius.xyz
VITE_HELIUS_API_KEY=
